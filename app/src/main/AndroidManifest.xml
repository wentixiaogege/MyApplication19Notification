<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jack.myapplication19notification" >

    <uses-permission android:name="com.android.alarm.permission.SET_ALARM"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MoreNotification"
            android:label="More Notification"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PRARENT_ACTIVITY"
                android:value=".MainActivity"/>

        </activity>
        <receiver android:name=".SetAlarmReceiver"></receiver>
    </application>

</manifest>
